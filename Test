var shirtWidth = 20;
var shirtLength = 29;
var shirtSleeve = 8.38;

if ((18<=shirtWidth<19.9) && (28<=shirtLength<28.9) && (8.13<=shirtSleeve<8.37)) {
    console.log("S");
} else if ((20<=shirtWidth<22) && (29<=shirtLength<30) && (8.38<=shirtSleeve<8.63)) {
    console.log("M");
} else if ((22<=shirtWidth<24) && (30<=shirtLength<31) && (8.63<=shirtSleeve<8.88)) {
    console.log("L");
} else if ((24<=shirtWidth<26) && (31<=shirtLength<33) && (8.88 <= shirtSleeve<9.63)) {
    console.log("XL");
} else if ((26<=shirtWidth<28) && (33 <=shirtLength<34) && (8.63<=shirtSleeve<10.13)) {
    console.log("2XL");
} else if (shirtWidth>= 28 && shirtLength>=34 && shirtSleeve>=10.13) {
    console.log("3XL");
} else {
    console.log("N/A");
}
